programa {
  funcao inicio() {
  cadeia nome
  cadeia idade
  inteiro cpf
  real altura
   escreva("informe seu nome completo: ")
    leia(nome)
    
    escreva("informe sua idade: ")
    leia(idade)
    se (idade < 0)
    escreva("idade invalida")
    senao 
    escreva("qual seu cpf? ")
    leia(cpf)
    
    escreva("informe sua altura; ")
    leia(altura)
    
    escreva("\nParabens, seu foi cadastro concliudo com sucesso, obrigado a preferencia!")
  
  }
}
